    "import pandas as pd\n",
    "import sqlite3\n",
    "\n",
    "df = pd.read_csv(\"cleansed_sales_1500.csv\")\n",
    "conn = sqlite3.connect(\"sales1500.db\")\n",
    "df.to_sql(\"sales\", conn, if_exists=\"replace\", index=False)\n",
    "conn.commit()\n",
    "conn.close()\n",
    "print(\"ETL to database completed.\")\n"
   ]
  }
